<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ArchivistGame.SeeScore">
    <ContentPage.Content>
        <Grid>
            <Frame BorderColor="Black">
                <StackLayout HorizontalOptions="FillAndExpand" VerticalOptions="CenterAndExpand" >
                    <Frame BorderColor="Black">
                        <Label HorizontalTextAlignment="Center" HorizontalOptions="FillAndExpand" x:Name="Points" FontSize="Large" Text="{Binding Total_points}"></Label>
                    </Frame>
                    <Label HorizontalTextAlignment="Center" HorizontalOptions="FillAndExpand" x:Name="AntalRigtige" Text="{Binding Antal_rigtige}"></Label>
                    <Label HorizontalTextAlignment="Center" HorizontalOptions="FillAndExpand" x:Name="ProcentRigtige" Text="{Binding Procent_rigtige}"></Label>
                    <Button x:Name="Afslut_Btn" HorizontalOptions="CenterAndExpand" BackgroundColor="LightGreen" Text="Afslut" Margin="0,50,0,0" Clicked="Afslut_Btn_Clicked"></Button>
                </StackLayout>
            </Frame>
         
        </Grid>
       

    </ContentPage.Content>
</ContentPage>